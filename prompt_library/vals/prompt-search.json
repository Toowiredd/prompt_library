{
  "name": "@toowired/prompt-search",
  "description": "Advanced search capabilities for prompts",
  "version": "1.0.0",
  "created": "2025-02-08 11:32:47",
  "author": {
    "github": "toowiredd",
    "valtown": "toowired"
  },
  "methods": {
    "fullTextSearch": {
      "input": {
        "query": "string",
        "options": {
          "fuzzy": "boolean",
          "weights": "object",
          "limit": "number"
        }
      },
      "output": "SearchResult[]"
    },
    "semanticSearch": {
      "input": {
        "query": "string",
        "threshold": "number"
      },
      "output": "SearchResult[]"
    },
    "tagSearch": {
      "input": {
        "tags": "string[]",
        "operator": "'AND' | 'OR'"
      },
      "output": "SearchResult[]"
    }
  }
}