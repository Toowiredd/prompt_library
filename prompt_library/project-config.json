  "structure": {
    "directories": {
      "scripts": "./scripts",
      "vals": "./vals",
      "docs": "./docs"
    }
  },
  "setup": {
    "steps": [
      {
        "order": 1,
        "name": "Clone Repository",
        "command": "git clone https://github.com/Toowiredd/prompt_library.git"
      },
      {
        "order": 2,
        "name": "Install Dependencies",
        "command": "npm install"
      },
      {
        "order": 3,
        "name": "Configure Environment",
        "command": "cp .env.example .env"
      },
      {
        "order": 4,
        "name": "Initialize Vals",
        "command": "npm run init"
      }
    ]
  }
}